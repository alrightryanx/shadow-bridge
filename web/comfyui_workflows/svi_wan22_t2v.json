{
  "name": "SVI Pro 2.0 + Wan 2.2 Text-to-Video",
  "description": "High quality text-to-video generation using SVI Pro 2.0 with Wan 2.2 enhancement",
  "version": "1.0",
  "input_type": "text",
  "nodes": {
    "1": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "{{PROMPT}}",
        "clip": ["2", 0]
      }
    },
    "2": {
      "class_type": "CheckpointLoaderSimple",
      "inputs": {
        "ckpt_name": "svi-pro-20/svi_pro_2.0.safetensors"
      }
    },
    "3": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "{{NEGATIVE_PROMPT}}",
        "clip": ["2", 0]
      }
    },
    "4": {
      "class_type": "EmptyLatentVideo",
      "inputs": {
        "width": "{{WIDTH}}",
        "height": "{{HEIGHT}}",
        "length": "{{FRAMES}}",
        "batch_size": 1
      }
    },
    "5": {
      "class_type": "KSampler",
      "inputs": {
        "model": ["2", 0],
        "positive": ["1", 0],
        "negative": ["3", 0],
        "latent_image": ["4", 0],
        "seed": "{{SEED}}",
        "steps": 30,
        "cfg": 7.5,
        "sampler_name": "euler",
        "scheduler": "normal",
        "denoise": 1.0
      }
    },
    "6": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["5", 0],
        "vae": ["2", 2]
      }
    },
    "7": {
      "class_type": "VHS_VideoCombine",
      "inputs": {
        "images": ["6", 0],
        "frame_rate": "{{FPS}}",
        "format": "video/mp4",
        "filename_prefix": "{{OUTPUT_PREFIX}}",
        "save_output": true
      }
    }
  },
  "defaults": {
    "NEGATIVE_PROMPT": "blurry, low quality, distorted, ugly, bad anatomy, watermark, text",
    "WIDTH": 1280,
    "HEIGHT": 720,
    "FPS": 24,
    "SEED": -1
  },
  "output_node": "7"
}
